{
  "name": "narrative-ai-generator",
  "version": "1.0.0",
  "description": "Sistema integral de generaciÃ³n narrativa con IA, conectado a n8n como backend",
  "private": true,
  "homepage": ".",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "react-router-dom": "^6.8.1",
    "axios": "^1.3.4",
    "lucide-react": "^0.263.1",
    "tailwindcss": "^3.2.7",
    "autoprefixer": "^10.4.14",
    "postcss": "^8.4.21",
    "uuid": "^9.0.0",
    "date-fns": "^2.29.3",
    "react-hot-toast": "^2.4.0",
    "framer-motion": "^10.12.4",
    "react-dropzone": "^14.2.3",
    "react-select": "^5.7.2",
    "react-textarea-autosize": "^8.4.1",
    "js-cookie": "^3.0.1",
    "lodash": "^4.17.21",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "analyze": "npm run build && npx source-map-explorer 'build/static/js/*.js'",
    "test:coverage": "npm test -- --coverage --watchAll=false",
    "test:ci": "npm test -- --coverage --watchAll=false --ci",
    "setup": "npm install && npm run build",
    "dev": "npm start"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn",
      "no-console": "warn",
      "prefer-const": "error",
      "react/prop-types": "off",
      "react/display-name": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/uuid": "^9.0.1",
    "@types/js-cookie": "^3.0.3",
    "@types/lodash": "^4.14.194",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.7",
    "source-map-explorer": "^2.5.3",
    "gh-pages": "^5.0.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@tailwindcss/aspect-ratio": "^0.4.2"
  },
  "keywords": [
    "narrative",
    "ai",
    "generator",
    "n8n",
    "react",
    "storytelling",
    "automation",
    "google-sheets",
    "series",
    "coherence",
    "creative-writing",
    "content-generation"
  ],
  "author": {
    "name": "Juan Carlos",
    "email": "juancspjr@example.com",
    "url": "https://github.com/juancspjr"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juancspjr/AC1.git"
  },
  "bugs": {
    "url": "https://github.com/juancspjr/AC1/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/**/*.test.{js,jsx}",
      "!src/setupTests.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  }
}